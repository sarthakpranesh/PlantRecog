    "react": "18.1.0",
    "react-native": "^0.70.0",
    "react-native-bootsplash": "^4.3.2",
    "react-native-camera": "^4.2.1",
    "react-native-gesture-handler": "^2.6.0",
    "react-native-image-picker": "^4.10.0",
